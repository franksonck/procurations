extends layout.pug

block content
  a.btn.btn-primary.pull-right(href="/logout") Se d√©connecter
  ul
    li Total&nbsp;: #{total}
  nav(aria-label="Page navigation")
    ul.pagination
      - var page = 1
      while page < total/100 + 1
        li
          a(href='/admin/' + page)= page++
  table.table
    tr
      th Email
      th Valide
      th Ville (Code INSEE)
    each elem in list
      tr
        td= elem.email
        td= elem.valid
        td= elem.city
